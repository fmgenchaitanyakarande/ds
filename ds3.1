def classify_income(income):
    if income < 40000:
        return 'Low Income'
    elif income > 80000:
        return 'High Income'
    else:
        return 'Moderate Income'

def categorize_age(age):
    if age < 25:
        return "Experienced"
    elif age > 18 and age <= 60:
        return "Fresher"
    else:
        return "Retired"

df['Income Category'] = df['Income'].apply(classify_income)
df['Age Category'] = df['Age'].apply(categorize_age)

df.sample(10)

